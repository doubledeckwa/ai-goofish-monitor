# --- AI Model related configuration ---
# modelAPI Key。
OPENAI_API_KEY=sk-...

# modelAPIinterface address. Here you need to fill in the information provided by the service provider、compatibleOpenAIFormattedAPIaddress
# View the large model you are usingAPIDocument, such as the format https://xx.xx.com/v1/chat/completions butOPENAI_BASE_URLJust fill in the first half https://xx.xx.com/v1/
OPENAI_BASE_URL=https://api-inference.modelscope.cn/v1/

# The model name used. The model needs to support image uploading.。
OPENAI_MODEL_NAME=XiaomiMiMo/MiMo-V2-Flash

# (Optional) forAIRequest configurationHTTP/Sacting. support http and socks5。For example: http://127.0.0.1:7890 or socks5://127.0.0.1:1080
PROXY_URL=""


# Whether the crawler runs in headless mode (defaulttrue)。
# usedockerDeployment not supportedGUI，set up RUN_HEADLESS=false Will cause failure to run。
RUN_HEADLESS=true

# AIdebug mode (defaultfalse)。After turning it on, more information will be printed on the console for troubleshooting.AIAnalyze problem logs。
AI_DEBUG_MODE=false

# Whether to enableenable_thinkingparameter (defaultfalse)。someAIModels require this parameter and some do not support it。
ENABLE_THINKING=false

# Whether to enableresponse_formatparameter (defaulttrue)。Bean bag model is not supportedjson_objectResponse format, needs to be set tofalse。Other models such asGeminiSupport can be set totrue。
ENABLE_RESPONSE_FORMAT=true

# Service port customization Not configured by default8000
SERVER_PORT=8000

# Administrator account password default admin/admin123
WEB_USERNAME=admin
WEB_PASSWORD=admin123

# Whether to useedgeBrowser kernel Used by defaultchromeBrowser kernel
LOGIN_IS_EDGE=false

# Whether to enable the conversion of computer links to mobile links Not enabled by default
PCURL_TO_MOBILE=true

# Agent rotation configuration
PROXY_ROTATION_ENABLED=false
PROXY_ROTATION_MODE="per_task" # per_task or on_failure
PROXY_POOL=""
PROXY_ROTATION_RETRY_LIMIT=2
PROXY_BLACKLIST_TTL=300


# ntfy Notification service configuration https://ntfy.sh/your-topic-name
NTFY_TOPIC_URL=

# (Optional) Gotify Notification service configuration
GOTIFY_URL=
GOTIFY_TOKEN=

# (Optional) Bark Notification service configuration，Bark push address, For example: https://api.day.app/your_key
BARK_URL=

# Enterprise WeChat robot notification configuration If none, no need to configure
WX_BOT_URL=

# Telegram Bot notification configuration If none, no need to configure
# get Bot Token: and @BotFather Conversations to create new bots
# get Chat ID: and @userinfobot Conversations get your usersID，Or add the bot to the group and get the groupID
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# (Optional) Universal Webhook Notification configuration your Webhook URL, For example: https://foo.bar.com/quz?a=b
WEBHOOK_URL=
# Request method: "GET" or "POST"
WEBHOOK_METHOD="POST"
# Custom request header (JSONFormat)
WEBHOOK_HEADERS='{"X-API-TOKEN":"your-secret-token"}'
# POSTRequest content type: "JSON" or "FORM"
WEBHOOK_CONTENT_TYPE="JSON"
# GETRequested query parameters (JSONFormat, support {{title}}, {{content}} placeholder)
WEBHOOK_QUERY_PARAMETERS='{"title":"{{title}}","content":"{{content}}"}'
# POSTRequest body (JSONFormat, support {{title}}, {{content}} placeholder)
WEBHOOK_BODY='{"title":"{{title}}","content":"{{content}}"}'


# Public Marketplace Configuration
# Enable public marketplace access
ENABLE_PUBLIC_MARKETPLACE=true

# JWT Configuration for user authentication
JWT_SECRET_KEY=your-secret-key-change-in-production
JWT_ALGORITHM=HS256
JWT_EXPIRE_HOURS=24

# Rate limiting for public API (requests per hour)
PUBLIC_API_RATE_LIMIT=100

# --- Feature Toggle Configuration ---
# Enable user registration (default: true)
ENABLE_USER_REGISTRATION=true

# Enable user login (default: true)  
ENABLE_USER_LOGIN=true

# Enable AI analysis (default: true)
ENABLE_AI_ANALYSIS=true

# Enable AI image analysis (default: true)
ENABLE_AI_IMAGE_ANALYSIS=true

# Enable enhanced anti-blocking (default: false)
ENABLE_ADVANCED_BLOCKING=false

# Enable favorites system (default: true)
ENABLE_FAVORITES_SYSTEM=true

# --- Image Processing Configuration ---
# Maximum images to use for AI analysis (default: 3)
MAX_IMAGES_FOR_AI=3

# Download all images or limit to AI count (default: true)
DOWNLOAD_ALL_IMAGES=true

# Image quality: low, medium, high (default: high)
IMAGE_QUALITY=high

# Concurrent image downloads (default: 5)
IMAGE_CONCURRENT_DOWNLOADS=5

# Image download timeout in seconds (default: 20)
IMAGE_TIMEOUT_SECONDS=20

# Image format: original, jpeg, webp (default: original)
IMAGE_FORMAT=original

# Maximum image size in MB (default: 10)
IMAGE_MAX_SIZE_MB=10
